{
  "name": "Host-Path Navigator",
  "version": "1.0",
  "description": "A simple extension to aid with repetitive navigation to multiple paths across multiple hostnames",
  "permissions": [
    "storage",
    "tabs",
    "downloads"
  ],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "windows": "Ctrl+B",
        "mac": "Command+B",
        "chromeos": "Ctrl+B",
        "linux": "Ctrl+B"
      }
    }
  },
  "manifest_version": 2
}